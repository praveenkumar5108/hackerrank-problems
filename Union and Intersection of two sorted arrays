def printUnion(arr1,arr2,m,n):
  i, j = 0, 0
  while i < m and j < n:
    if arr1[i] < arr2[j]:
      print(arr1[i])
      i += 1
   elif arr1[i] > arr2[j]:
      print(arr2[j]
      j += 1
   else:
     print(arr2[j])
     i += 1
     j += 1
  while i<m:
    print(arr1[i])
    i += 1
  while j<n:
    print(arr2[j])
    j += 1
arr1 = [1,2,3,5]
arr2 = [2,4,6,7]
m = len(arr1)
n = len(arr2)
printUnion(arr1,arr2,m,n)
